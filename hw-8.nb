Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<`*\>\"", "]"}], "\n", 
 RowBox[{"Text", "[", 
  "\"\<First, I will show that the monopole term is zero. This is done simply \
by integrating to find the total charge in the sphere and showing that it is \
equal to zero. \>\"", "]"}], "\n", 
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"k", " ", 
     RowBox[{"R", "/", 
      RowBox[{"r", "^", "2"}]}], 
     RowBox[{"(", 
      RowBox[{"R", "-", 
       RowBox[{"2", "r"}]}], ")"}], 
     RowBox[{"Sin", "[", "\[Theta]", "]"}], 
